{
	"git.addGBlameToVirtualText": true,
	"explorer.width": 30,
	"explorer.icon.enableNerdfont": true,
	"explorer.previewAction.onHover": false,
	"explorer.keyMappings.global": {
		"<cr>": [
			"expandable?",
			"expand",
			"open"
		],
		"v": "open:vsplit"
	},
	"languageserver": {
		"golang": {
			"command": "gopls",
			"rootPatterns": [
				"go.mod"
			],
			"trace.server": "verbose",
			"filetypes": [
				"go"
			]
		}
  },
  "clangd.path": "/root/.config/coc/extensions/coc-clangd-data/install/11.0.0/clangd_11.0.0/bin/clangd"
}
